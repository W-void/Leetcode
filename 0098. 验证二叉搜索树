
/**
 * Definition for a binary tree node.
 * struct TreeNode {
 *     int val;
 *     TreeNode *left;
 *     TreeNode *right;
 *     TreeNode(int x) : val(x), left(NULL), right(NULL) {}
 * };
 */
class Solution {
public:
    bool isValidBST(TreeNode* root) {
        //中序遍历
        stack<TreeNode*> s;
        long min = LONG_MIN;
        while(root != NULL || !s.empty()){
            while(root != NULL){
                s.push(root);
                root = root->left;
            }
            if(!s.empty()){
                root = s.top();
                if(min >= root->val) return false;
                min = root->val;
                s.pop();
                root = root->right;
                
            }
        }
        return true;
    }
};
